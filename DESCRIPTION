Package: transfR
Type: Package
Title: Transfer of Hydrograph from Gauged to Ungauged Catchments
Version: 1.0.11
Date: 2023-10-02
Authors@R: c(
  person("Alban", "de Lavenne", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-9448-3490"), email = "alban.delavenne@inrae.fr"),
